{
    "name": "asaycomm/emptor-php-api",
    "description": "Vodafone Emptor Telco PHP API",
    "type": "library",
    "keywords": [
        "vodafone",
        "emptor",
        "php",
        "web service"
    ],
    "authors": [
        {
            "name": "asaycomm",
            "email": "arge@asay.com.tr"
        }
    ],
    "require": {
