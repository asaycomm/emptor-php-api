        "monolog/monolog": "^1.24",
        "fabpot/goutte": "^3.2",
        "simple-html-dom/simple-html-dom": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Emptor\\Corporate\\": "src/Corporate/",
